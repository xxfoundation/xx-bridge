{
  "name": "xxbridge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite --port 9296",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "_lint": "eslint --max-warnings=0 --ignore-path .gitignore",
    "lint:check": "npm run-script _lint -- -- .",
    "lint:fix": "npm run-script _lint -- --fix -- .",
    "prettier:check": "prettier --check -- .",
    "prettier:fix": "prettier --write -- .",
    "tsc:check": "concurrently --timings -m 2 -n 'tsc_react,tsc_root' 'tsc --project tsconfig.json' 'tsc --project tsconfig.root.json'",
    "_qa:check": "concurrently --timings -m 3 -n 'tsc_all,eslint,prettier' -P 'npm run-script tsc:check' 'npm run-script _lint -- -- {@}' 'prettier --check -- {@}' --",
    "qa:check": "npm run-script _qa:check -- .",
    "qa:fix": "npm run-script lint:fix && npm run-script prettier:fix"
  },
  "dependencies": {
    "@apollo/client": "^3.9.9",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.17",
    "@mui/system": "^5.15.5",
    "@mui/x-charts": "^6.19.1",
    "@polkadot/api": "9.14.2",
    "@polkadot/extension-dapp": "^0.44.9",
    "@polkadot/extension-inject": "^0.44.9",
    "@polkadot/keyring": "10.4.2",
    "@polkadot/types": "9.14.2",
    "@polkadot/util": "10.4.2",
    "@reduxjs/toolkit": "^2.2.3",
    "@tanstack/react-query": "^5.32.0",
    "@web3modal/wagmi": "^4.1.11",
    "@xxnetwork/types": "^1.0.4",
    "graphql": "^16.8.1",
    "graphql-ws": "^5.16.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.18.0",
    "redux-localstorage": "^0.4.1",
    "viem": "^2.9.25",
    "wagmi": "^2.5.20"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "prettier": "^3.2.4",
    "sass": "^1.69.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-node-polyfills": "^0.19.0"
  },
  "lint-staged": {
    "*.{json,js,ts,jsx,tsx}": [
      "npm run-script _lint -- --fix --",
      "prettier --write --"
    ]
  }
}
